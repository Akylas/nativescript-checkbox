{
  "name": "@nstudio/nativescript-checkbox",
  "version": "1.0.0",
  "description": "NativeScript plugin for checkbox widget.",
  "main": "checkbox",
  "typings": "index.d.ts",
  "nativescript": {
    "platforms": {
      "android": "6.0.0",
      "ios": "6.0.0"
    }
  },
  "scripts": {
    "build": "npm i && tsc",
    "prepublishOnly": "npm run build && npm run build.ng.module",
    "build.ng.module": "node ./node_modules/.bin/ngc --project tsconfig.aot.json --outDir ./",
    "tslint": "cd .. && tslint \"**/*.ts\" --config tslint.json --exclude \"**/node_modules/**\" --exclude \"*demo*/platforms/**\" --exclude \"**/typings/**\"",
    "ci.tslint": "npm i && tslint '**/*.ts' --config '../tslint.json' --exclude '**/node_modules/**' --exclude \"**/typings/**\"",
    "demo.ios": "npm run setup && cd ../demo && tns run ios --no-hmr",
    "demo.android": "npm run setup && cd ../demo && tns run android --no-hmr",
    "demo.ng.ios": "cd ../demo-ng && tns run ios --no-hmr --env.aot",
    "demo.ng.android": "cd ../demo-ng && tns run android --no-hmr --env.aot",
    "setup": "npm i && tsc",
    "generate.typings.ios": "cd ../demo && TNS_DEBUG_METADATA_PATH=\"$(pwd)/metadata\" tns build ios && TNS_TYPESCRIPT_DECLARATIONS_PATH=\"$(pwd)/typings\" tns build ios && echo 'Now look for your library typings in demo/typings!'",
    "make-changelog": "cd ../ && github_changelog_generator -u nstudio -p nativescript-videoplayer"
  },
  "lint-staged": {
    "**/*.{js, css, ts, json, scss, html, xml, md}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run tslint"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nstudio/nativescript-checkbox.git"
  },
  "keywords": [
    "NativeScript",
    "TypeScript",
    "Android",
    "iOS",
    "Material Design",
    "CheckBox",
    "nStudio"
  ],
  "author": {
    "name": "Brad Martin",
    "email": "bradwaynemartin@gmail.com"
  },
  "bugs": {
    "url": "https://github.com/nstudio/nativescript-checkbox/issues"
  },
  "license": "MIT",
  "homepage": "https://github.com/nstudio/nativescript-checkbox",
  "readmeFilename": "README.md",
  "devDependencies": {
    "@angular/common": "~10.0.2",
    "@angular/compiler": "~10.0.2",
    "@angular/compiler-cli": "~10.0.2",
    "@angular/core": "~10.0.2",
    "@angular/forms": "~10.0.2",
    "@nativescript/angular": "^9.0.0",
    "@nativescript/core": "^6.5.8",
    "rxjs": "~6.6.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5",
    "tns-platform-declarations": "^6.5.8",
    "rimraf": "^2.6.3",
    "tslint": "~6.0.0",
    "typescript": "~3.9.2",
    "zone.js": "~0.9.1",
    "prompt": "^1.0.0"
  }
}
